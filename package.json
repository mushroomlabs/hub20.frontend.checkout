{
  "name": "vue-hub20-checkout",
  "version": "0.0.5",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve --watch",
    "build": "vue-cli-service build --target lib --name hub20 ./lib/main.js",
    "dev": "vue-cli-service build --target lib --name hub20 ./lib/main.js --watch",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:debug": "node --inspect-brk node_modules/.bin/vue-cli-service test:unit --runInBand --watchAll",
    "lint": "vue-cli-service lint",
    "cypress:open": "cypress open"
  },
  "main": "./dist/hub20.umd.js",
  "dependencies": {
    "decimal.js-light": "^2.5.0",
    "hub20-vue-sdk": "^0.1",
    "jshashes": "^1.0.7",
    "qrcode": "^1.4.4",
    "vue": "^2.6.10",
    "vue-moment": "^4.1.0",
    "vue-simple-progress": "^1.1.1",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.0.3",
    "cypress": "^7.2.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.42.1",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.10",
    "zenstyles": "^0.0.1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "preset": "@vue/cli-plugin-unit-jest"
  }
}
